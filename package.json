    "@vicons/fa": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons4": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vicons/tabler": "^0.12.0",
    "axios": "^1.1.3",
    "casdoor-vue-sdk": "^1.3.0",
    "jsdom": "^20.0.1",
    "jwt-decode": "^3.1.2",
    "naive-ui": "^2.33.5",
    "pinia": "^2.0.23",
    "showdown": "^2.1.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
